            <name>
                <lang xml:lang="en">LED</lang>
              </name>
                  <description>
                <lang xml:lang="en"></lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="en">The LED is set to turn ON when the connected light is turned OFF. This allows the LED to indicate the switch’s location in a dark room.</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="en">To make the LED turn ON when the light is turned ON, change parameter 3’s value to “1”</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="4" type="constant" size="1" default="00">
