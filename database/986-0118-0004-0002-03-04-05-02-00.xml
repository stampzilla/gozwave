    <configParams>
        <configParam number="1" type="constant" size="1" default="01">
            <name>
                <lang xml:lang="en">Change the state of indicator light</lang>
              </name>
                  <description>
                <lang xml:lang="en"></lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="en">LED indicator will be &quot;On&quot;</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="en">LED indicator will be &quot;Off&quot;</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="2" type="constant" size="1" default="01">
            <name>
                <lang xml:lang="en">Memory function</lang>
              </name>
                  <description>
                <lang xml:lang="en"></lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="en">memory function is not open</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="en">memory function is open</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="3" type="range" size="2" default="02d0">
            <name>
                <lang xml:lang="en">Watt Meter Report Period</lang>
              </name>
                  <description>
                <lang xml:lang="en"></lang>
              </description>
      
                  <value from="0001" to="7fff" unit="s">
                <description>
                    <lang xml:lang="en">example: 5*720s = 3600s = 1 hour</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="4" type="range" size="2" default="0006">
            <name>
                <lang xml:lang="en">KWH Meter Report Period</lang>
              </name>
                  <description>
                <lang xml:lang="en"></lang>
              </description>
      
                  <value from="0001" to="7fff" unit="min">
                <description>
                    <lang xml:lang="en">example: 6*10min = 1 hour</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="5" type="range" size="2" default="0bb8">
            <name>
                <lang xml:lang="en">Threshold of Watt for Load Caution</lang>
              </name>
                  <description>
                <lang xml:lang="en"></lang>
              </description>
      
                  <value from="000a" to="0bb8" unit="Watt">
                <description>
                    <lang xml:lang="en"></lang>
