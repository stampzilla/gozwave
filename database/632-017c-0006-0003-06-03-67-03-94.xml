    <configParams>
        <configParam number="1" type="range" size="1" default="03">
            <name>
                <lang xml:lang="en">Missing Radio Link time</lang>
              </name>
                  <description>
                <lang xml:lang="en">Sets the maximum amount of time the device will wait for reconnection
before going to emergency mode (All switches
ON).</lang>
              </description>
      
                  <value from="01" to="18">
                <description>
                    <lang xml:lang="en">It is possible to set a range of 10min to 4hours. Default value is 30min. The time interval is set in 10 min steps.</lang>
                  </description>
              </value>
                </configParam>
        <configParam number="2" type="constant" size="1" default="00">
            <name>
                <lang xml:lang="en">Relay Type</lang>
              </name>
                  <description>
                <lang xml:lang="en">Sets relay type. Default relay
type is normal open.</lang>
              </description>
      
                  <value from="00" to="00">
                <description>
                    <lang xml:lang="en">0 = normal open</lang>
                  </description>
              </value>
            <value from="01" to="01">
                <description>
                    <lang xml:lang="en">1 = normal close</lang>
                  </description>
              </value>
            <value from="02" to="02">
                <description>
                    <lang xml:lang="en">2 = switching</lang>
                  </description>
              </value>
                </configParam>
